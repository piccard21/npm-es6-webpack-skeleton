{
  "name": "npm-es6-webpack-skeleton",
  "version": "0.0.2",
  "description": "A simple skeleton for npm-module-development with ES6, based on webpack.",
  "main": "dist/app.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack-dev-server --open --config webpack.dev.js"
  },
  "keywords": [
    "npm",
    "node",
    "webpack",
    "es6",
    "webpack",
    "skeleton"
  ],
  "author": "Andreas Stephan <info@cafe-serendipity.com> (cafe-serendipity.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17", 
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1", 
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
